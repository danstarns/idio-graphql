{
  "name": "idio-graphql",
  "version": "4.0.0",
  "description": "Node.js library for splitting SDL first GraphQL schemas into composable & idiomatic chunks.",
  "main": "./src/index.js",
  "types": "./index.d.ts",
  "scripts": {
    "test": "./node_modules/.bin/cross-env ./node_modules/.bin/mocha",
    "test:dev": "./node_modules/.bin/cross-env ./node_modules/.bin/mocha --watch",
    "test:coverage": "./node_modules/.bin/cross-env ./node_modules/.bin/nyc ./node_modules/.bin/mocha",
    "docs:start": "cd ./website && docusaurus-start ",
    "docs:build": "cd ./website && docusaurus-build",
    "publish-gh-pages": "cd ./website && docusaurus-publish",
    "docs:version": "cd ./website && docusaurus-version",
    "docs:rename-version": "cd ./website && docusaurus-rename-version "
  },
  "keywords": [
    "graphql",
    "modularize",
    "modules",
    "schema",
    "microservices",
    "modular"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": "github:danstarns/idio-graphql",
  "homepage": "https://github.com/danstarns/idio-graphql",
  "author": "Daniel Starns",
  "license": "MIT",
  "devDependencies": {
    "@graphql-tools/utils": "^6.0.18",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "apollo-server": "^2.16.1",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "docusaurus": "^1.14.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "graphql-tag": "^2.11.0",
    "graphql-type-json": "^0.3.2",
    "mocha": "^8.1.1",
    "nats": "^1.4.12",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "proxyquire": "^2.1.3"
  },
  "optionalDependencies": {
    "moleculer": "^0.14.8"
  },
  "peerDependencies": {
    "graphql": "^0.13.0 || ^14.0.0 || ^15.0.0"
  },
  "dependencies": {
    "@graphql-toolkit/schema-merging": "^0.10.7",
    "@graphql-tools/schema": "^6.0.18",
    "for-emit-of": "^1.3.3",
    "graphql": "^15.3.0",
    "safe-json-stringify": "^1.2.0",
    "uuid": "^8.3.0"
  }
}
